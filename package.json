    "generator-mocha": "^0.2.0",
    "mkdirp": "^0.5.1",
    "underscore.string": "^3.2.2",
    "wiredep": "^2.2.2",
    "yeoman-assert": "^2.1.0",
    "yeoman-generator": "^0.21.1",
    "yosay": "^1.1.0"
  },
  "devDependencies": {
    "blanket": "^1.2.1",
